{
  "name": "sniper-bot",
  "version": "5.0.0",
  "type": "module",
  "main": "./out/sniper.js",
  "types": "./src/types/types.d.ts",
  "exports": {
    ".": {
      "import": "./out/src/sniper.js",
      "require": "./out/src/sniper.js"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MajesticString/sniper.git"
  },
  "scripts": {
    "dev": "tsup --watch --onSuccess \"node . --experimental-json-modules\"",
    "build": "tsup",
    "watch": "tsup --watch",
    "start": "pm2 start ./out/sniper.js",
    "dev:start": "npm run build && node ./out/sniper.js",
    "register-commands": "npm run build && env ONLY_UPDATE_COMMANDS=y node ./out/sniper.js",
    "lint": "eslint . --ext .ts,.js",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@discordjs/rest": "^0.3.0",
    "@discordjs/voice": "^0.8.0",
    "@sapphire/discord.js-utilities": "^4.9.3",
    "@sapphire/fetch": "^2.3.0",
    "@sapphire/framework": "^2.4.1",
    "@sapphire/type": "^2.2.0",
    "@sapphire/utilities": "^3.6.1",
    "@top-gg/sdk": "^3.1.3",
    "cheerio": "^1.0.0-rc.10",
    "colorette": "^2.0.16",
    "discord-api-types": "^0.30.0",
    "discord.js": "^13.6.0",
    "firebase-admin": "^10.0.2",
    "lodash-es": "^4.17.21",
    "luxon": "^2.3.1",
    "ms": "^2.1.3",
    "sharp": "^0.30.3",
    "topgg-autoposter": "^2.0.1"
  },
  "devDependencies": {
    "@firebase/app-compat": "0.1.21",
    "@firebase/app-types": "0.7.0",
    "@types/body-parser": "1.19.2",
    "@types/lodash-es": "4.17.6",
    "@types/luxon": "^2.3.1",
    "@types/ms": "0.7.31",
    "@types/node": "17.0.23",
    "@types/prettier": "2.4.4",
    "@types/sharp": "0.30.0",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-node": "11.1.0",
    "nodemon": "2.0.15",
    "pm2": "^5.2.0",
    "prettier": "2.6.0",
    "ts-node": "10.7.0",
    "tsup": "5.12.1",
    "typescript": "4.6.3"
  },
  "engines": {
    "node": ">=17.0.0"
  }
}
