{
  "name": "sniper-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "docs:dev": "npm run docs:update && vuepress dev docs",
    "docs:dev:simple": "vuepress dev docs",
    "docs:build": "npm run docs:update && vuepress build docs",
    "docs:build:simple": "vuepress build docs",
    "docs:update": "cd apps/sniper && npm run register-commands && cd ../.. && node scripts/update-docs.js"
  },
  "devDependencies": {
    "@discord-message-components/core": "^0.2.1",
    "@sapphire/fetch": "^2.4.0",
    "@types/glob": "^7.2.0",
    "@types/node": "18.0.3",
    "@vuepress/client": "2.0.0-beta.49",
    "@vuepress/plugin-google-analytics": "2.0.0-beta.48",
    "@vuepress/plugin-search": "2.0.0-beta.49",
    "@vuepress/plugin-shiki": "2.0.0-beta.48",
    "@vuepress/plugin-toc": "2.0.0-beta.49",
    "cheerio": "^1.0.0-rc.11",
    "colorette": "^2.0.17",
    "eslint": "8.19.0",
    "eslint-plugin-vue": "^9.1.0",
    "glob": "^8.0.3",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "pnpm": "^7.1.8",
    "postcss": "^8.4.14",
    "prettier": "2.7.1",
    "turbo": "1.3.1",
    "typescript": "4.7.4",
    "vue": "^3.2.36",
    "vuepress-vite": "2.0.0-beta.49"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.6.0"
  },
  "packageManager": "pnpm@6.29.1"
}
