{
  "name": "sniper",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsup --watch --onSuccess \"DEV_SCRIPT=true node --enable-source-maps .\"",
    "build": "tsup",
    "watch": "tsup --watch",
    "start": "pnpm build && node dist/index.js",
    "lint": "eslint --ext .ts,js .",
    "pm2:start": "pnpm build && pm2 start dist/index.js",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "docs:dev": "vitepress dev docs",
    "docs:dev:update": "pnpm run docs:update && vitepress dev docs",
    "docs:dev:update:existing": "node scripts/update-docs.js && vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:build:update": "node scripts/add-fake-config.js && npm run docs:update && vitepress build docs",
    "docs:build:update:existing": "node scripts/update-docs.js && vitepress build docs",
    "docs:update": "pnpm build && ONLY_UPDATE_DOCS=true DEV_SCRIPT=true node dist/index.js && node scripts/update-docs.js && pnpm run format"
  },
  "dependencies": {
    "@firebase/app-compat": "^0.1.38",
    "@kaname-png/plugin-statcord": "^2.1.5",
    "@sapphire/decorators": "5.1.0-pr-434.9fcf7df.0",
    "@sapphire/discord.js-utilities": "5.1.0-pr-434.9fcf7df.0",
    "@sapphire/framework": "3.2.0-pr-512.244de39.0",
    "@sapphire/plugin-logger": "^3.0.1",
    "@sapphire/type": "^2.2.4",
    "@sapphire/utilities": "3.10.2-pr-434.9fcf7df.0",
    "colorette": "^2.0.19",
    "discord.js": "^14.6.0",
    "firebase-admin": "^11.2.0",
    "lodash-es": "^4.17.21",
    "luxon": "^3.1.0",
    "ms": "3.0.0-canary.1"
  },
  "devDependencies": {
    "@firebase/app-types": "^0.8.1",
    "@sapphire/eslint-config": "^4.3.8",
    "@types/luxon": "^3.0.2",
    "@types/node": "18.11.8",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "pnpm": "^7.14.1",
    "prettier": "2.7.1",
    "tsup": "^6.3.0",
    "typescript": "4.9.4",
    "vitepress": "1.0.0-alpha.30",
    "vue": "^3.2.41"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.6.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/react",
        "react",
        "react-dom",
        "@algolia/client-search"
      ]
    }
  },
  "packageManager": "pnpm@7.19.0"
}
