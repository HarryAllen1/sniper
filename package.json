{
  "name": "sniper",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "tsup --watch --onSuccess \"DEV_SCRIPT=true node --enable-source-maps .\"",
    "build": "tsup",
    "watch": "tsup --watch",
    "start": "pnpm build && node dist/index.js",
    "lint": "eslint --ext .ts,js .",
    "pm2:start": "pnpm build && pm2 start dist/index.js",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "docs:dev": "vitepress dev docs",
    "docs:dev:update": "pnpm run docs:update && vitepress dev docs",
    "docs:dev:update:existing": "node scripts/update-docs.js && vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:build:update": "npm run docs:update && vitepress build docs",
    "docs:build:update:existing": "node scripts/update-docs.js && vitepress build docs",
    "docs:update": "echo no"
  },
  "dependencies": {
    "@firebase/app-compat": "^0.1.32",
    "@kaname-png/plugin-statcord": "^2.1.3",
    "@sapphire/decorators": "5.0.1-pr-434.549335e.0",
    "@sapphire/discord.js-utilities": "5.1.0-pr-434.549335e.0",
    "@sapphire/framework": "3.1.0-pr-512.135ce4a.0",
    "@sapphire/plugin-logger": "^3.0.0",
    "@sapphire/type": "^2.2.4",
    "@sapphire/utilities": "^3.9.2",
    "colorette": "^2.0.19",
    "discord.js": "^14.3.0",
    "firebase-admin": "^11.0.1",
    "luxon": "^3.0.3",
    "ms": "3.0.0-canary.1"
  },
  "devDependencies": {
    "@firebase/app-types": "^0.7.0",
    "@sapphire/eslint-config": "^4.3.8",
    "@types/luxon": "^3.0.0",
    "@types/node": "18.7.14",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "eslint": "8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "lodash": "^4.17.21",
    "pnpm": "^7.9.5",
    "prettier": "2.7.1",
    "tsup": "^6.2.3",
    "typescript": "4.8.2",
    "vitepress": "1.0.0-alpha.13",
    "vue": "^3.2.38"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.6.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/react",
        "react",
        "react-dom",
        "@algolia/client-search"
      ]
    }
  },
  "packageManager": "pnpm@7.9.5"
}
