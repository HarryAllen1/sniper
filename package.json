{
  "name": "sniper-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "docs:dev": "npm run docs:update && vuepress dev docs",
    "docs:build": "npm run docs:update && vuepress build docs",
    "docs:update": "cd apps/sniper && npm run register-commands && cd ../.. && ts-node scripts/update-docs.ts"
  },
  "devDependencies": {
    "@discord-message-components/core": "^0.2.1",
    "@sapphire/fetch": "^2.4.0",
    "@skyra/discord-components-core": "^3.4.0",
    "@types/glob": "^7.2.0",
    "@types/node": "17.0.36",
    "@vuepress/client": "2.0.0-beta.45",
    "@vuepress/plugin-google-analytics": "^2.0.0-beta.46",
    "@vuepress/plugin-search": "^2.0.0-beta.46",
    "@vuepress/plugin-shiki": "^2.0.0-beta.46",
    "@vuepress/plugin-toc": "^2.0.0-beta.46",
    "cheerio": "^1.0.0-rc.11",
    "colorette": "^2.0.17",
    "eslint": "8.16.0",
    "eslint-plugin-vue": "^9.1.0",
    "glob": "^8.0.3",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "pnpm": "^7.1.8",
    "postcss": "^8.4.14",
    "prettier": "2.6.2",
    "ts-node": "10.8.0",
    "turbo": "1.2.14",
    "typescript": "4.7.2",
    "vue": "^3.2.36",
    "vuepress": "^2.0.0-beta.46",
    "webpack": "^5.73.0"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.6.0"
  },
  "packageManager": "pnpm@6.29.1"
}
