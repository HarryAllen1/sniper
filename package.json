{
  "name": "sniper-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "docs:dev": "npm run docs:update && vuepress dev docs",
    "docs:build": "npm run docs:update && vuepress build docs",
    "docs:update": "cd apps/sniper && npm run register-commands && cd ../.. && ts-node scripts/update-docs.ts"
  },
  "devDependencies": {
    "@discord-message-components/core": "^0.2.1",
    "@sapphire/fetch": "^2.3.1",
    "@skyra/discord-components-core": "^3.3.1",
    "@types/glob": "^7.2.0",
    "@types/node": "17.0.29",
    "@vuepress/plugin-google-analytics": "^2.0.0-beta.41",
    "@vuepress/plugin-search": "^2.0.0-beta.41",
    "@vuepress/plugin-shiki": "^2.0.0-beta.41",
    "@vuepress/plugin-toc": "^2.0.0-beta.41",
    "cheerio": "^1.0.0-rc.10",
    "colorette": "^2.0.16",
    "eslint": "8.14.0",
    "eslint-plugin-vue": "^8.7.1",
    "glob": "^8.0.1",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "pnpm": "^6.32.10",
    "prettier": "2.6.2",
    "ts-node": "10.7.0",
    "turbo": "1.2.5",
    "typescript": "4.6.4",
    "vue": "^3.2.33",
    "vuepress": "^2.0.0-beta.41",
    "webpack": "^5.72.0"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.6.0"
  },
  "packageManager": "pnpm@6.29.1"
}
