{
  "name": "sniper-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "update": "ts-node update-commands.ts"
  },
  "devDependencies": {
    "@discord-message-components/core": "^0.2.1",
    "@discord-message-components/vue": "^0.2.1",
    "@sapphire/fetch": "^2.1.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/node": "17.0.21",
    "@vuepress/plugin-google-analytics": "^2.0.0-beta.35",
    "@vuepress/plugin-pwa": "^2.0.0-beta.35",
    "@vuepress/plugin-pwa-popup": "^2.0.0-beta.35",
    "@vuepress/plugin-search": "^2.0.0-beta.35",
    "@vuepress/plugin-shiki": "^2.0.0-beta.35",
    "@vuepress/plugin-toc": "^2.0.0-beta.35",
    "cheerio": "^1.0.0-rc.10",
    "eslint": "8.10.0",
    "eslint-plugin-vue": "^8.4.1",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "prettier": "2.5.1",
    "ts-node": "10.5.0",
    "turbo": "1.1.5",
    "typescript": "4.6.2",
    "vue": "^3.2.31",
    "vuepress": "^2.0.0-beta.35",
    "webpack": "^5.69.1"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.6.0"
  },
  "packageManager": "pnpm@6.29.1"
}
