{
  "name": "sniper",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsup --watch --onSuccess \"DEV_SCRIPT=true node --enable-source-maps .\"",
    "build": "tsup",
    "watch": "tsup --watch",
    "start": "pnpm build && node dist/index.js",
    "lint": "eslint --ext .ts,js .",
    "pm2:start": "pnpm build && pm2 start dist/index.js",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "docs:dev": "vitepress dev docs",
    "docs:dev:update": "pnpm run docs:update && vitepress dev docs",
    "docs:dev:update:existing": "node scripts/update-docs.js && vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:build:update": "node scripts/add-fake-config.js && npm run docs:update && vitepress build docs",
    "docs:build:update:existing": "node scripts/update-docs.js && vitepress build docs",
    "docs:update": "pnpm build && ONLY_UPDATE_DOCS=true DEV_SCRIPT=true node dist/index.js && node scripts/update-docs.js && pnpm run format"
  },
  "dependencies": {
    "@firebase/app-compat": "^0.2.0",
    "@kaname-png/plugin-statcord": "^2.1.10",
    "@sapphire/decorators": "6.0.0",
    "@sapphire/discord.js-utilities": "6.0.4",
    "@sapphire/framework": "4.2.2",
    "@sapphire/plugin-logger": "^3.0.1",
    "@sapphire/type": "^2.3.0",
    "@sapphire/utilities": "3.11.0",
    "colorette": "^2.0.19",
    "discord.js": "^14.7.1",
    "firebase-admin": "^11.4.1",
    "lodash-es": "^4.17.21",
    "luxon": "^3.2.1",
    "ms": "3.0.0-canary.1"
  },
  "devDependencies": {
    "@firebase/app-types": "^0.9.0",
    "@sapphire/eslint-config": "^4.3.8",
    "@types/lodash-es": "^4.17.6",
    "@types/luxon": "^3.2.0",
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "eslint": "8.36.0",
    "eslint-config-prettier": "^8.6.0",
    "pnpm": "^7.23.0",
    "prettier": "2.8.7",
    "tsup": "^6.5.0",
    "typescript": "4.9.5",
    "vitepress": "1.0.0-alpha.63",
    "vue": "^3.2.45"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.6.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/react",
        "react",
        "react-dom",
        "@algolia/client-search"
      ]
    }
  },
  "packageManager": "pnpm@7.30.5"
}
