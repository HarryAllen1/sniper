{
  "name": "sniper-bot",
  "version": "5.1.3",
  "type": "module",
  "main": "./out/sniper.js",
  "types": "./src/types/types.d.ts",
  "exports": {
    ".": {
      "import": "./out/src/sniper.js",
      "require": "./out/src/sniper.js"
    }
  },
  "imports": {
    "#fetch": "./dist/utils/helpers/fetch.js"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MajesticString/sniper.git"
  },
  "scripts": {
    "dev": "tsup --watch --onSuccess \"node . --experimental-json-modules --enable-source-maps\"",
    "build": "tsup",
    "watch": "tsup --watch",
    "start": "npm run build && pm2 start ./out/sniper.js --node-args=\"--enable-source-maps\"",
    "dev:start": "npm run build && node ./out/sniper.js --enable-source-maps",
    "register-commands": "node ./createNoopConfig.js && npm run build && env ONLY_UPDATE_COMMANDS=y node ./out/sniper.js --enable-source-maps",
    "lint": "eslint . --ext .ts,.js",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@discordjs/builders": "^1.1.0",
    "@discordjs/rest": "^1.0.1",
    "@sapphire/discord.js-utilities": "^5.0.0",
    "@sapphire/utilities": "^3.9.2",
    "@top-gg/sdk": "^3.1.3",
    "colorette": "^2.0.17",
    "discord-api-types": "^0.37.3",
    "discord.js": "^13.10.2",
    "firebase-admin": "^11.0.1",
    "lodash-es": "^4.17.21",
    "luxon": "^3.0.1",
    "ms": "^2.1.3",
    "topgg-autoposter": "^2.0.1",
    "undici": "^5.9.1"
  },
  "devDependencies": {
    "@firebase/app-compat": "0.1.32",
    "@firebase/app-types": "0.7.0",
    "@types/lodash-es": "4.17.6",
    "@types/luxon": "^3.0.0",
    "@types/ms": "0.7.31",
    "@types/node": "18.7.8",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-node": "11.1.0",
    "pm2": "^5.2.0",
    "prettier": "2.7.1",
    "tsup": "6.2.2",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": ">=17.0.0"
  }
}
